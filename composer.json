{
  "name": "sagalbot/encryptable",
  "description": "Allows you to store Eloquent properties encrypted in your database, and automatically decrypt when accessed.",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Sagalbot\\Encryptable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "illuminate/database": "^5.4",
    "illuminate/encryption": "^5.4"
  },
  "keywords": [
    "laravel", "laravel 5", "eloquent", "encryption"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jeff Sagal",
      "email": "sagalbot@gmail.com"
    }
  ]
}
